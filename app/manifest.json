{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/virtual-desktop-icon.png",
    "128": "images/virtual-desktop-icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["./scripts/websocket.js", "./scripts/RTCMultiConnection.min.js", "./scripts/CodecsHandler.js", "./scripts/desktop-capturing.js"],
    "persistent": false
  },
  "permissions": [
    "tabs", "desktopCapture", "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/virtual-desktop-icon.png",
      "38": "images/virtual-desktop-icon.png"
    },
    "default_title": "Smart Desktop"
  }                 ,
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
